	"tokenColors": [
		{
			"scope": ["string.quoted.double.json", "string.quoted.single.json"],
			"settings": { "foreground": "#CBA6F7" }
		},
		{
			"scope": [
			"punctuation.definition.dictionary.begin.json",
			"punctuation.definition.dictionary.end.json",
			"punctuation.separator.dictionary.key-value.json",
			"punctuation.separator.dictionary.pair.json"
			],
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": ["support.type.property-name.json"],
			"settings": { "foreground": "#9AD4F7" }
		},
		{
			"scope": ["constant.numeric.json", "constant.language.json"],
			"settings": { "foreground": "#89DCEB" }
		},
		{
			"scope": ["constant.language.boolean.json", "constant.language.null.json"],
			"settings": { "foreground": "#A6BDF7" }
		},

		{
			"scope": [
			"storage.type.primitive.cpp",
			"storage.type.built-in.primitive.cpp"
			],
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": "keyword.type.primitive.cpp",
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": [
			"punctuation.section.block.begin.bracket.curly.function.definition.cpp",
