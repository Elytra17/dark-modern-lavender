			"punctuation.section.block.end.bracket.curly.cpp",
			"punctuation.section.parameters.begin.bracket.round.cpp",
			"punctuation.section.parameters.end.bracket.round.cpp",
			"punctuation.section.parens.begin.bracket.round.cpp",
			"punctuation.section.parens.end.bracket.round.cpp"
			],
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": "string.quoted.other.lt-gt.include.cpp",
			"settings": { "foreground": "#CBA6F7" }
		},
		{
			"scope": "comment.line.double-slash.cpp",
			"settings": { "foreground": "#96b1d6" }
		},
		{
			"scope": "comment.block.cpp",
			"settings": { "foreground": "#96b1d6" }
		},
		{
			"scope": "entity.name.type.cpp",
			"settings": { "foreground": "#9AD4F7" }
		},
		{
			"scope": "keyword.control.if.cpp",
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": "keyword.control.else.cpp",
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": "keyword.control.while.cpp",
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": "constant.numeric.cpp",
			"settings": { "foreground": "#bfd0fd" }
		},
		{
			"scope": "constant.numeric.decimal.cpp",
			"settings": { "foreground": "#bfd0fd" }
		},
		{
			"scope": "keyword.control.directive.include.cpp",
			"settings": { "foreground": "#bfd0fd" }
		},

		{
			"scope": "keyword.control.cs",
			"settings": { "foreground": "#A6BDF7" }
		},
		{
			"scope": "storage.type.cs",
			"settings": { "foreground": "#9AD4F7" }
		},
		{
			"scope": "entity.name.type.class.cs",
			"settings": { "foreground": "#9AD4F7" }
		},
		{
			"scope": "variable.parameter.cs",
			"settings": { "foreground": "#CBA6F7" }
		},
		{
			"scope": "string.quoted.double.cs",
			"settings": { "foreground": "#CBA6F7" }
		},
		{
			"scope": "comment.line.double-slash.cs",
			"settings": { "foreground": "#96b1d6" }
		},
		{
			"scope": "constant.numeric.cs",
			"settings": { "foreground": "#89DCEB" }
		},
		{
			"scope": "entity.name.function.cs",
			"settings": { "foreground": "#B4E0FF" }
		},
		{
			"scope": "punctuation.terminator.statement.cs",
			"settings": { "foreground": "#ffffff" }
		}
